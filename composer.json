{
    "name": "othyn/go-calendar",
    "type": "project",
    "description": "A highly convenient auto updated community calendar for Pokemon GO in iCal format, powered by LeekDuck.",
    "keywords": [
        "calendar",
        "tool",
        "pokemon"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "spatie/icalendar-generator": "^2.5",
        "symfony/console": "^6.2",
        "friendsofphp/php-cs-fixer": "^3.13",
        "nesbot/carbon": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Console\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "php-cs-fixer": "./vendor/bin/php-cs-fixer fix --allow-risky yes --config ./.php-cs-fixer.dist.php"
    }
}
